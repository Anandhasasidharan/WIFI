<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include all shared preferences -->
        <include domain="sharedpref" path="." />
        <!-- Exclude sensitive data -->
        <exclude domain="database" path="wifimonitor.db" />
    </cloud-backup>
    <device-transfer>
        <!-- Include all shared preferences -->
        <include domain="sharedpref" path="." />
        <!-- Exclude sensitive data -->
        <exclude domain="database" path="wifimonitor.db" />
    </device-transfer>
</data-extraction-rules>